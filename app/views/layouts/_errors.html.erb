<% if object.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= "#{pluralize(object.errors.count, "error")} #{t('activerecord.errors.header', record: object.class.model_name.human.downcase)}" %>
    </h2>

    <ul>
    <% object.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>